<?xml version="1.0" encoding="UTF-8"?>
<Procedure xmlns="http://codac.iter.org/sup/sequencer" version="1.0"
           name="Trivial procedure for testing purposes"
           xmlns:xs="http://www.w3.org/2001/XMLSchema-instance"
           xs:schemaLocation="http://codac.iter.org/sup/sequencer sequencer.xsd">
    <Declaration>
        <ComplexCondition>
            <Sequence>
                <MathExpressionNode expression="$param1"/>
                <Condition var_name="$param2"/>
            </Sequence>
        </ComplexCondition>
    </Declaration>
    <Sequence>
        <ComplexCondition name="ComplexCondition" param1="c:=a+2*b+c" param2="c" />
        <IncludeNode path="../resources/workspace.Sequence.Only"/>
    </Sequence>
    <Workspace>
        <Local name="a"
               type='{"type":"int8"}'
               value='1' />
        <Local name="b"
               type='{"type":"uint8"}'
               value='2' />
        <Local name="c"
               type='{"type":"int32"}'                              
               value='0' />
    </Workspace>
</Procedure>

