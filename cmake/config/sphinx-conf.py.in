extensions = [ "breathe" ]
breathe_projects = { "sequencer-plugin-mathexper": "@CMAKE_CURRENT_BINARY_DIR@/xml" }
breathe_default_project = "sequencer-plugin-mathexper"

project = "sequencer-plugin-mathexper"
author = "Ricardo Torres"
copyright = "ITER Organization"
version = "@PROJECT_VERSION@"
release = "@PROJECT_VERSION@"

# Set link name generated in the top bar.
html_title = 'SEQUENCER PLUGIN MATHEXPER'

try:
    import sphinx_rtd_theme
    html_theme = 'sphinx_rtd_theme'
except ImportError:
    import sys
    sys.stderr.write('Warning: Read The Docs theme is not available.\n')

# Set syntax highlighting default to C++.
highlight_language = 'cpp'

# Set primary domain to C++.
primary_domain = 'cpp'
